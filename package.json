{
  "name": "filtered",
  "version": "0.0.1",
  "description": "Text based filter and search for firebase firestore and array of objects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.browser.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "preversion": "npm run test",
    "test": "jest",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "firebase",
    "firestore",
    "filter",
    "search",
    "query"
  ],
  "author": "Maye Edwin",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.6.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "typescript": "^3.9.7"
  }
}
